<div class="highlight">
    <div>
        <div id="mainContent">
            <h2>
                <%= post.title %>
            </h2>
            <p>
                <%= post.content %>
            </p>
        </div>
        <div>
            <div id="postUser">
                <h4>
                    <%= user.profileName %>
                </h4>
                <p>
                    <% post.tags.forEach(tag=> { %>
                        <span>
                            <%= tag %>,
                        </span>
                        <% }) %>
                </p>
            </div>
        </div>
    </div>
    <div id="interactionSection">
        <div id="likingDiv" class="interactionDiv">
            <p>liked</p>
        </div>
        <div id="votingDiv" class="interactionDiv">
            <p>voted</p>
        </div>
    </div>
</div>